def p(x):
    return (x%2).bit_and(x>1)

arr = [1,2,0,9,4,7,-9]
sarr = sint.Array(len(arr))
for i in range(len(arr)):
    sarr[i] = arr[i]

out = count_satisfied(sarr,p)
out.reveal()
flag = out == 2

@if_e(flag.reveal())
def _():
    print_ln("Pass the test")
@else_
def _():
    pass
